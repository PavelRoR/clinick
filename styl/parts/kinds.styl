#kinds
	margin-top: 80px
	overflow hidden
.tile
	background-color: grey
	border-radius: 5px
	box-shadow: 0px 2px 5px 0px rgba(0, 0, 0, 0.2);
	width: 100%
	height: 421px
	text-align: center
	position relative
	margin-bottom: 60px
	img 
		width: inherit
	h4
		margin: 20px 0 10px
	p
		font-size: 18px
		padding: 0 20px
		line-height: 27px
	.button
		position absolute
		left: 26.5%;
		bottom: 28px
		&:hover,
		&:focus
			color: #fff