*
	margin: 0;
	padding: 0
	outline: 0
	text-decoration: none

h1
	display: inline-block
	font-size: 35px;
	font-family: "OpenSansBold", sans-serif;
	color: rgb(255, 255, 255);
	line-height: 1.2;
	text-align: center;
	background-color: green
	border-radius: 50px
	padding: 23px 0px
	margin: 80px 0 30px;
	width: 650px
	float: right
	clear: right

h2
	display: inline-block
	font-size: 24px;
	font-family: "RobotoRegular",sans-serif;
	color: #fafbfb;
	line-height: 1.2;
	text-align: center;
	background-color: #3b3c3c
	border-radius: 50px
	padding: 7px 80px
	width: 650px
	margin-bottom: 77px
	float: right
	clear: right


h3
	font-size: 30px;
	font-family: "OpenSansBold", sans-serif;
	color: h3_color
	line-height: 1.2;
	text-align: center;
	margin-top: 100px
	&:after
		content: url(../img/pluses.png)
		display: block;
		width: 65px;
		height: 16px;
		margin: 0 auto;

h4
	font-size: 24px;
	font-family: "OpenSansBold", sans-serif;
	color: h3_color
	font-weight: bold;
	line-height: 1.2;
	text-align: center;
p
	font-family: "OpenSans", sans-serif;
	font-size: 16px
	color: p_black

.button
	border: 1px solid #198050
	border-radius: 19px;
	background-image: linear-gradient( 90deg, #29a46b 0%, #1d8f5a 100%);
	padding: 12px 25px
	color: #ffffff
	font-size: 13px;
	font-family: "OpenSansBold", sans-serif;
	text-transform: uppercase;
	line-height: 1.2;
	text-align: center;
	cursor pointer
	&:hover
		background-image: none
		border-color: #2e2f2f
		border-radius: 20px;
		background-image: linear-gradient( 90deg, #4f5150 0%, #3b3c3c 100%);
		
a
	&:hover
	&:focus
		text-decoration: none
		color: initial
		outline: 0
#first_screen
	width 100%
	background-image: url(../img/first_screen_back.jpg)
	background-position: center top
	background-repeat: no-repeat
	background-size cover
select
	&:focus
	&:active
		outline: 0;
		border: 0


#thanks
	h1
		margin: 30px auto
		float: none
		display: block
		padding: 23px 20px
		width: 650px
	#first_screen
		height: calc(100vh - 215px)

