#footer
	background-color: green 
	padding: 40px 0 25px
	text-align: center
	clear both
	margin-top: 20px
	h5
		font-size: 14px;
		font-family: "OpenSansBold", sans-serif;
		color: #fff
		text-transform: uppercase;
	p
		font-size: 14px;
		font-family: "OpenSans", sans-serif;
		color: #fff
		line-height: 20px
		margin-bottom: 15px
	.col-md-4:nth-child(2)
		p
			text-transform: uppercase
.soc_buttons
	margin: 30px auto 0;
	width: 52%;
	a
		&:nth-child(2)
			margin: 0 30px
		.fa
			font-size: 23px;		
			color: green
			background-color: #fff
			border-radius: 10px;
			height: 30px;
			width: 30px
			padding-top: 4px;
.up
	display: none;
	position: fixed;
	bottom: 20px;
	right: calc(50% - 630px);
	text-align: center;
	text-decoration: none;
	transition: 400ms;
	background-color: transparent
	border-radius: 50%;
	.fa
		border-radius: 50%;
		display block
		color: green
		font-size: 54px;
		box-shadow: 0px 3px 8px 2px rgba(0,0,0,0.3)
		background-color: #fff
		height: 54px;
		width: 54px;
	&:hover
		color: h3_color
	&:focus
		color: green
#menu_icon 
	display: none;
	position: fixed;
	top: 33px;
	right: 33px;
	width: 20px;
	height: 20px;
	background-color: #fff;
	box-shadow: 0px 3px 6px 0px rgba(0, 0, 0, 0.25);
	width: 63px;
	height: 63px;
	z-index: 999;
	cursor: pointer;
	overflow: hidden;


.upper_span,
.middle_span,
.lower_span 
	background-color: rgb(111, 200, 46);
	width: 45px;
	height: 9px;
	display: block;
	position: absolute;
	left: 9px;
	-webkit-transition: all .4s ease;
	-o-transition: all .4s ease;
	transition: all .4s ease;


.upper_span
	top: 9px;


.upper_span_opened
	top: 29px;
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	-ms-transform: rotate(45deg);
	-o-transform: rotate(45deg);
	transform: rotate(45deg);


.middle_span
	top: 27px;


.middle_span_opened
	-webkit-transform: translateX(-60px);
	-moz-transform: translateX(-60px);
	-ms-transform: translateX(-60px);
	-o-transform: translateX(-60px);
	transform: translateX(-60px);


.lower_span
	top: 45px;


.lower_span_opened
	top: 29px;
	-webkit-transform: rotate(-45deg);
	-moz-transform: rotate(-45deg);
	-ms-transform: rotate(-45deg);
	-o-transform: rotate(-45deg);
	transform: rotate(-45deg);
